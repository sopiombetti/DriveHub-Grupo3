{
  "entryPoints": ["src"],
  "entryPointStrategy": "expand",
  "tsconfig": "tsconfig.json",
  "out": "documentacion",

  "readme": "none",

  "exclude": [
    "**/__tests__/**",
    "**/*.spec.ts",
    "**/*.test.ts"
